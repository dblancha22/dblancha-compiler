COMMENT //tests the for/print keyword and while keyword
IDENTIFIER x
COLON :
INTEGER integer
SEMICOLON ;
FOR for
OPEN_PAREN (
IDENTIFIER x
EQUALS =
INTEGER_LITERAL 0
SEMICOLON ;
IDENTIFIER x
LT <
INTEGER_LITERAL 5
SEMICOLON ;
IDENTIFIER x
INCREMENT ++
CLOSE_PAREN )
OPEN_CURLY {
PRINT print
STRING_LITERAL counting: 
NOT ,
IDENTIFIER x
SEMICOLON ;
CLOSE_CURLY }
IDENTIFIER a
COLON :
INTEGER integer
EQUALS =
INTEGER_LITERAL 0
SEMICOLON ;
WHILE while
OPEN_PAREN (
IDENTIFIER a
GT >
INTEGER_LITERAL 0
CLOSE_PAREN )
OPEN_CURLY {
IDENTIFIER a
DECREMENT --
SEMICOLON ;
CLOSE_CURLY }
